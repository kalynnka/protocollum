# Development environment configuration for protocollum
# Copy this file to .env.local and modify as needed

# Database Configuration
DATABASE_URL=postgresql://protocollum:dev_password@localhost:5432/protocollum_dev
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=protocollum_dev
DATABASE_USER=protocollum
DATABASE_PASSWORD=dev_password

# Development Settings
DEBUG=true
LOG_LEVEL=DEBUG

# pgAdmin Configuration (when using --profile admin)
PGADMIN_DEFAULT_EMAIL=admin@protocollum.local
PGADMIN_DEFAULT_PASSWORD=admin_password